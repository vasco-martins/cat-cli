{
    "name": "vascomartins/cat-cli",
    "description": "The CLI for the Cat Framework.",
    "keywords": ["cat-cli", "console", "cli"],
    "homepage": "https://github.com/vasco-martins/cat-cli",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/vasco-martins/cat-cli/issues",
        "source": "https://github.com/vasco-martins/cat-cli"
    },
    "authors": [
        {
            "name": "Vasco Martins",
            "email": "vascomartins37@gmail.com"
        },
        {
            "name": "Miguel Ferreira",
            "email": "miguel.personal@protonmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "laminas/laminas-text": "^2.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^0.3",
        "laravel-zero/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["build/cat"]
}
